{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-max-props-per-line": [2, { "maximum": { "single": 3, "multi": 1}}],
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off"
  }
}
